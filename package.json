{
    "name":    "svelte-app",
    "version": "0.1.0",
    "private": true,
    "license": "MIT",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "svelte": "./dist/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev":    "rollup -c -w",
        "prod":   "run-s svelte lint build",
        "build":  "rollup -c && mv stats-*.html doc/",
        "start":  "sirv public --no-clear --host 0.0.0.0",
        "svelte": "svelte-check --tsconfig ./tsconfig.json",
        "lint":   "prettier --check --plugin-search-dir=. ./src && echo Checking ESLint rules... && eslint ./src",
        "format": "prettier --write --plugin-search-dir=. ./src",
        "package": "svelte-package",
        "prepublishOnly": "npm run package"
    },
    "_altDependencies": {
        "rollup-plugin-terser": "7.0.2",
        "semantic-ui-css": "2.4.1",
        "svelte": "4.0.0",
        "eslint-plugin-svelte": "2.31.1"
    },
    "dependencies": {
        "svelte": "3.59.1",
        "svelte-spa-router": "3.3.0",
        "semantic-ui-css": "2.5.0",
        "fomantic-ui-css": "2.9.2",
        "jquery": "3.7.0",
        "yup": "1.2.0"
    },
    "devDependencies": {
        "rollup": "3.25.3",
        "rollup-plugin-css-only": "4.3.0",
        "rollup-plugin-livereload": "2.0.5",
        "rollup-plugin-sizes": "1.0.5",
        "rollup-plugin-svelte": "7.1.6",
        "rollup-plugin-visualizer": "5.9.2",
        "@rollup/plugin-commonjs": "25.0.2",
        "@rollup/plugin-node-resolve": "15.1.0",
        "@rollup/plugin-terser": "0.4.3",
        "@rollup/plugin-typescript": "11.1.1",

        "eslint": "8.43.0",
        "eslint-plugin-svelte3": "4.0.0",
        "@typescript-eslint/eslint-plugin": "5.60.1",
        "@typescript-eslint/parser": "5.60.1",

        "eslint-config-airbnb-typescript": "17.0.0",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-jsx-a11y": "6.7.1",
        "eslint-plugin-promise": "6.1.1",
        "eslint-plugin-react": "7.32.2",

        "prettier": "2.8.8",
        "prettier-plugin-svelte": "2.10.1",

        "@sveltejs/package": "2.1.0",
        "svelte-check": "3.4.4",
        "svelte-preprocess": "5.0.4",
        "typescript": "5.1.3",
        "tslib": "2.6.0",
        "@types/jquery": "3.5.16",

        "npm-run-all": "4.1.5",
        "sirv-cli": "2.0.2"
    }
}
